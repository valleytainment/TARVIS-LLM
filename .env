# REQUIRED (Update if using cloud sync)
AWS_ACCESS_KEY_ID=your_aws_key_placeholder
AWS_SECRET_ACCESS_KEY=your_aws_secret_placeholder

# OPTIONAL
USE_GPU=0 # Set to 1 if compatible GPU is available
HIGH_ACCURACY_MODE=0 # Set to 1 for potentially better responses at cost of performance
GOOGLE_DRIVE_CREDENTIALS_FILE= # Path to Google Drive API credentials JSON file (if using GDrive sync)
STORAGE_MODE=local # Options: local, google_drive

